{
  "name": "next-with-tailwindcss",
  "version": "1.0.0",
  "description": "NextJS with Tailwind CSS",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "npm run test:format && npm run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:format": "prettier --check **/*.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lailo-ch/next-with-tailwindcss.git"
  },
  "author": "Lailo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lailo-ch/next-with-tailwindcss/issues"
  },
  "homepage": "https://github.com/lailo-ch/next-with-tailwindcss#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "^9.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.10.2",
    "tailwindcss": "^1.1.2",
    "typescript": "^3.6.3"
  }
}
