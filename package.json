{
  "name": "next-with-tailwindcss",
  "version": "1.0.0",
  "description": "NextJS with Tailwind CSS",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "npm run test:format && npm run test:unit",
    "test:unit": "jest",
    "test:format": "prettier --check **/*.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lailo-ch/next-with-tailwindcss.git"
  },
  "author": "Lailo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lailo-ch/next-with-tailwindcss/issues"
  },
  "homepage": "https://github.com/lailo-ch/next-with-tailwindcss#readme",
  "dependencies": {
    "next": "^9.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "tailwindcss": "^1.0.6",
    "typescript": "^3.5.3"
  }
}
